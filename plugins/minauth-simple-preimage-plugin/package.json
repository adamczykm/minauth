  "main": "dist/index.js",
  "browser": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "install-ci": "npm install --unsafe-perm=true",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc './**/*.ts' --write",
    "test": "echo \"Error: no tests here\" && exit 0"
  },
  "dependencies": {
